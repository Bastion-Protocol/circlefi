# Environment Configuration
# Copy this file to .env and fill in your values

# Private key for contract deployment (without 0x prefix)
PRIVATE_KEY=your_private_key_here

# RPC URLs
AVALANCHE_RPC_URL=https://api.avax.network/ext/bc/C/rpc
FUJI_RPC_URL=https://api.avax-test.network/ext/bc/C/rpc

# Explorer API Keys
SNOWTRACE_API_KEY=your_snowtrace_api_key

# Frontend Configuration
NEXT_PUBLIC_WALLET_CONNECT_PROJECT_ID=your_walletconnect_project_id
NEXT_PUBLIC_ENVIRONMENT=development

# Contract Addresses (filled after deployment)
NEXT_PUBLIC_CIRCLE_FI_POOL=
NEXT_PUBLIC_DOMA_COLLATERAL=
NEXT_PUBLIC_MOCK_USDC=
NEXT_PUBLIC_YELLOW_INTEGRATION=

# Yellow SDK Configuration
YELLOW_API_KEY=your_yellow_api_key
YELLOW_WEBHOOK_SECRET=your_yellow_webhook_secret

# Database (if needed for off-chain data)
DATABASE_URL=postgresql://localhost:5432/circlefi

# Security
JWT_SECRET=your_jwt_secret_here
ENCRYPTION_KEY=your_encryption_key_here